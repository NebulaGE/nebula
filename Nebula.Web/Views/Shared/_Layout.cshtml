            <span class="loader-icon loader-icon_5"></span>
            <span class="loader-icon loader-icon_6"></span>
            <span class="loader-icon loader-icon_7"></span>
        </div>
    </div>
    
    <div class="FrodDropAllBlock"></div>
    <div id="mask" style="position: fixed; top: 0; z-index: 3; background: black; display: none;"></div>
    
    <link href="~/Content/Responsive-Youtube-Vimeo-Video-Lightbox-Plugin-YouTubePopUp/YouTubePopUp.css" rel="stylesheet" />
    <script src="~/Content/Responsive-Youtube-Vimeo-Video-Lightbox-Plugin-YouTubePopUp/YouTubePopUp.jquery.js"></script>
    <script type="text/javascript">
        jQuery(function () {
            jQuery("a.bla-1").YouTubePopUp();
        });
    </script>

    @RenderSection("phoneScripts", required: false)
    <script src="http://cdn.mathjax.org/mathjax/2.2-latest/MathJax.js?config=TeX-AMS_HTML"></script>

    @Scripts.Render("~/bundles/mainJs") 
    <script src="~/Scripts/mathjax.min.js"></script>
    <link href="~/Content/jquery-confirm.min.css" rel="stylesheet" />
    <script src="~/Js/common/utils.js"></script>
    <div id="fb-root"></div>
  
    <script src="~/Content/assetsNew/js/my.js"></script>

    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-36251023-1']);
        _gaq.push(['_setDomainName', 'jqueryscript.net']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script');
            ga.type = 'text/javascript';
            ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') +
                '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ga, s);
        })();
    </script>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5&appId=579191162230603";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));

        (function () {
            var tempDataReturnUrl = '@TempData["ReturnUrl"]';
            if (tempDataReturnUrl) {
                $('#return-url').val(tempDataReturnUrl);
                $('.returnUrl').val(tempDataReturnUrl);
            }
            var tempData = '@TempData["show-login-form"]';
            if (tempData) {
                $('.mainReg').fadeIn();
                showLoginForm();
            }

            var externalLoginTempData = '@TempData["external-login-confirmation"]';
            if (externalLoginTempData) {
                $('.mainReg').fadeIn();
                $('.register-form, #aut-form, #reg-form,.login-form').hide();
                $('#email-confirm-form').show();
            }

            var externalLoginErrors = '@Html.Raw(TempData["errors"])';
            if (externalLoginErrors) {
                var externalLoginErrosTempData = JSON.parse(externalLoginErrors);

                if (externalLoginErrosTempData) {
                    if (externalLoginErrosTempData.length) {
                        var errorString = '<div style="text-align:center;">';
                        $.each(externalLoginErrosTempData,
                            function (i, v) {
                                errorString += '<p class="error-class">' + v + '</p>';
                            });
                        errorString += '</div>';

                        $.alert({
                            title: '',
                            content: errorString,
                            confirmButton: 'დახურვა',
                            columnClass: 'col-md-4 col-md-offset-4'
                        });
                    }
                }
            }

            var pdfTempData = '@TempData["show-pdf-popup"]';
            if (pdfTempData) {
                showPdfPopup(true);
            }

            var transactionSuccess = '@TempData["transaction-success"]';

            if (transactionSuccess) {
                $.alert({
                    title: 'გილოცავთ!',
                    content: 'ტრანზაქცია წარმატებით განხორციელდა',
                    confirmButton: 'დახურვა',
                    columnClass: 'col-md-4 col-md-offset-4'
                });
            }

            var transactionError = '@TempData["transaction-error"]';
            if (transactionError) {
                var err =
                    'სამწუხაროდ ტრანზაქცია ვერ განხორციელდა, გთხოვთ მოგვწერეთ ელ.ფოსტაზე : info@nebula.ge ან  <a href="https://www.facebook.com/nebula.ge" target="_blank">Facebook-ზე</a>';
                $.alert({
                    title: '',
                    content: err,
                    confirmButton: 'დახურვა',
                    columnClass: 'col-md-4 col-md-offset-4'
                });
            }

            var licenseTempData = '@TempData["already-has-license"]';
            if (licenseTempData) {
                $.alert({
                    title: '',
                    content: 'თქვენ უკვე შეძენილი გაქვთ ლიცენზია',
                    confirmButton: 'დახურვა',
                    columnClass: 'col-md-4 col-md-offset-4'
                });
            }

            var forgotPasswordTempData = '@TempData["Code"]';
            if (forgotPasswordTempData) {
                $('.forgot-password').fadeIn();
            }

        })();

        setInterval(function () {
                $.get(U + 'home/asd');
            },
            200000);

        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] ||
                function () {
                    (i[r].q = i[r].q || []).push(arguments);
                }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m);
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-75760788-1', 'auto');
        ga('send', 'pageview');
    </script>

    @RenderSection("scripts", required: false)

</body>
</html>
